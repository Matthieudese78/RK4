C DYNE23    SOURCE    CB215821  20/11/25    13:26:28     10792          
      SUBROUTINE DYNE23(ICHP,KTPAS,I1)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
*--------------------------------------------------------------------*
*                                                                    *
*     Opérateur DYNE : algorithme de Fu - de Vogelaere               *
*     ________________________________________________               *
*                                                                    *
*     Remplissage du tableau des variables généralisées.             *
*                                                                    *
*     Paramètres:                                                    *
*                                                                    *
* e   ICHP  : CHPOINT dans lequel on va chercher les valeurs         *
* es  KTPAS : Segment des variables au cours d'un pas de temps       *
* e   I1    : Indice du pas                                          *
*                                                                    *
*     Auteur, date de création:                                      *
*                                                                    *
*     Lionel VIVAN, le 18 Septembre 1989.                            *
*                                                                    *
*--------------------------------------------------------------------*
-INC SMCHPOI
-INC PPARAM
*
      SEGMENT,MTPAS
         REAL*8 FTOTA(NA1,4),FTOTB(NPLB,IDIMB),FTOTBA(NA1)
         REAL*8 XPTB(NPLB,2,IDIMB),FINERT(NA1,4)
         REAL*8 XVALA(NLIAA,4,NTVAR),XVALB(NLIAB,4,NTVAR)
         REAL*8 FEXB(NPLB,3,IDIM),XCHPFB(2,NLIAB,4,NPLB)
      ENDSEGMENT
*
      MTPAS = KTPAS
      IA = 0
      MCHPOI = ICHP
      SEGACT,MCHPOI
      NSOUPO = IPCHP(/1)
      DO 10 II1=1,NSOUPO
         MSOUPO = IPCHP(II1)
         SEGACT,MSOUPO
         NC = NOCOMP(/2)
         MPOVAL = IPOVAL
         SEGACT,MPOVAL
         N = VPOCHA(/1)
         DO 20 J=1,N
            DO 21 K=1,NC
               IA = IA+1
               FTOTA(IA,I1) = VPOCHA(J,K)
 21      CONTINUE
 20      CONTINUE
         SEGDES,MPOVAL,MSOUPO
 10   CONTINUE
      SEGDES,MCHPOI
*
      RETURN
      END


 
 
 
